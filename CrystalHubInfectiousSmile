coroutine.wrap(KDDAQ_fake_script)()
local function DEZPMRF_fake_script() -- GravityFill.LocalScript 
	local script = Instance.new('LocalScript', GravityFill)

	local UserInputService = game:GetService("UserInputService")
	local Dragging = false
	script.Parent.lens.MouseButton1Down:Connect(function()
		Dragging = true
	end)
	
	UserInputService.InputChanged:Connect(function()
		if Dragging then
			local MousePos = UserInputService:GetMouseLocation()+Vector2.new(0)
			local RelPos = MousePos-script.Parent.AbsolutePosition
			local Precent = math.clamp(RelPos.X/script.Parent.AbsoluteSize.X,0,1)
			script.Parent.lens.Position = UDim2.new(Precent-0.035, 0,-2.08, 0)
			workspace.Gravity = Precent*110
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			Dragging = false
		end
	end)
end
