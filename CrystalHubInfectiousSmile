DiscordCopyB.MouseButton1Click:Connect(function()
	setclipboard('https://discord.gg/FVsQSFZx8k')

	wait(0.5)

	game.StarterGui:SetCore("SendNotification",  {
		Title = "CrystalHub";
		Text = "Link Copied. Go Your Browser And Paste";
		Duration = 5;
		Callback = NotificationBindable;
	})

end)

