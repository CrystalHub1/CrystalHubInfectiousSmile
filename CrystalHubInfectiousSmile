local function LBAQGOD_fake_script() -- NoclipB.ButtonEffect 
	local script = Instance.new('LocalScript', NoclipB)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(10, 116, 65)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(18, 208, 119)}):Play()
	end)
	script.Parent.MouseButton1Click:Connect(function()
		noclip = false
		game:GetService('RunService').Stepped:Connect(function()
			if noclip then
				game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
	
			end
		end)
	
		local p = game.Players.LocalPlayer
		local mo = p:GetMouse()
	
		mo.KeyDown:Connect(function(k)
			if k == 'e' then
				noclip = not noclip
				p.Character.Humanoid:ChangeState(11)
			end
		end)
	end)
end
