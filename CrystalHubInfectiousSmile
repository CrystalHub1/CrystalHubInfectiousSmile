-- Scripts:

local function EYIIUT_fake_script() -- CrystalHubV2Main.Dragify 
	local script = Instance.new('LocalScript', CrystalHubV2Main)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		local dragSpeed = 0
		dragInput = nil
		dragStart = nil
		local dragPos = nil
		function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(EYIIUT_fake_script)()
local function KMZNX_fake_script() -- CrystalHubV2Main.Open/Close 
	local script = Instance.new('LocalScript', CrystalHubV2Main)

	p = game.Players.LocalPlayer
	mouse = p:GetMouse()
	
	mouse.KeyDown:connect(function(key)
		if key == "m" then
			if script.Parent.Visible == true then
				script.Parent.Visible = false
			else
				script.Parent.Visible = true
			end
		end
	end)
end
coroutine.wrap(KMZNX_fake_script)()
local function TKUZBM_fake_script() -- CrystalHubV2Main.ExecuteEffect 
	local script = Instance.new('LocalScript', CrystalHubV2Main)

	wait(1)
	
	script.Parent:TweenPosition(UDim2.new(0.274, 0,0.171, 0), "Out", "Bounce", 2)
end
coroutine.wrap(TKUZBM_fake_script)()
local function BCKEE_fake_script() -- DiscordB.LocalScript 
	local script = Instance.new('LocalScript', DiscordB)

	local Mouse = game.Players.LocalPlayer:GetMouse()
	local button = script.Parent
	local SK = false
	local Zone = script.Parent.Effect2
	local SpeedS = 1
	local TweenService = game:GetService("TweenService") 
	local tweenClick = TweenInfo.new(SpeedS,Enum.EasingStyle.Quint,Enum.EasingDirection.Out,0,false,0)
	
	button.MouseButton1Click:connect(function()	
		local G = Zone:Clone()
		G.Position = UDim2.new(0,Mouse.X-button.AbsolutePosition.X,0,Mouse.Y-button.AbsolutePosition.Y)
		G.Parent = button
		local tween = TweenService:Create(G, tweenClick, {Size = UDim2.new(1,0,1,0),ImageTransparency = 1})
		tween:Play()
		wait(SpeedS)
		G:Destroy()
	end)
end
coroutine.wrap(BCKEE_fake_script)()
local function IBXY_fake_script() -- DiscordB.LocalScript 
	local script = Instance.new('LocalScript', DiscordB)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(10, 116, 65)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(18, 208, 119)}):Play()
	end)
	script.Parent.MouseButton1Click:Connect(function()
		--script
	end)
end
coroutine.wrap(IBXY_fake_script)()
local function ZIETL_fake_script() -- DiscordCopyB.LocalScript 
	local script = Instance.new('LocalScript', DiscordCopyB)

	local Mouse = game.Players.LocalPlayer:GetMouse()
	local button = script.Parent
	local SK = false
	local Zone = script.Parent.Click
	local SpeedS = 1
	local TweenService = game:GetService("TweenService") 
	local tweenClick = TweenInfo.new(SpeedS,Enum.EasingStyle.Quint,Enum.EasingDirection.Out,0,false,0)
	
	button.MouseButton1Click:connect(function()	
		local G = Zone:Clone()
		G.Position = UDim2.new(0,Mouse.X-button.AbsolutePosition.X,0,Mouse.Y-button.AbsolutePosition.Y)
		G.Parent = button
		local tween = TweenService:Create(G, tweenClick, {Size = UDim2.new(1,0,1,0),ImageTransparency = 1})
		tween:Play()
		wait(SpeedS)
		G:Destroy()
	end)
end
coroutine.wrap(ZIETL_fake_script)()
